# ResuBoost AI Frontend - Staging Environment Configuration
# Copy this file to .env.local for local development with staging settings

# =============================================================================
# API Configuration
# =============================================================================
NEXT_PUBLIC_API_URL=https://api-staging.resuboost.ai
NEXT_PUBLIC_WS_URL=wss://api-staging.resuboost.ai/ws

# =============================================================================
# Application Settings
# =============================================================================
NEXT_PUBLIC_APP_NAME=ResuBoost AI (Staging)
NEXT_PUBLIC_APP_VERSION=2.0.0-staging
NEXT_PUBLIC_ENVIRONMENT=staging

# =============================================================================
# Feature Flags
# =============================================================================
NEXT_PUBLIC_ENABLE_DEBUG=true
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_MOCK_DATA=false

# =============================================================================
# Third-Party Services (Staging)
# =============================================================================
# Sentry (Error Tracking)
NEXT_PUBLIC_SENTRY_DSN=
SENTRY_AUTH_TOKEN=

# Analytics (Disabled in staging)
NEXT_PUBLIC_GA_MEASUREMENT_ID=

# =============================================================================
# Build Configuration
# =============================================================================
NEXT_TELEMETRY_DISABLED=1
NODE_ENV=production

# =============================================================================
# Rate Limiting / Performance
# =============================================================================
NEXT_PUBLIC_API_TIMEOUT=30000
NEXT_PUBLIC_RETRY_ATTEMPTS=3
